<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/night1_word"
        android:orientation="vertical">

        <include
            android:id="@+id/in_title"
            layout="@layout/normal_title" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@color/allbackground"
            android:orientation="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_marginTop="@dimen/dpDimen50"
                android:layout_height="@dimen/dpDimen30">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dpDimen20"
                    android:text="手机号码"
                    android:textColor="#ff9e9e9e"
                    android:textSize="15sp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:text="13800138000"
                    android:layout_marginRight="@dimen/dpDimen20"
                    android:textColor="#ffe8d2b0"
                    android:textSize="15sp"
                    />
                <View
                    android:layout_width="match_parent"
                    android:layout_alignParentBottom="true"
                    android:layout_height="@dimen/dpDimen1"
                    android:layout_marginLeft="@dimen/dpDimen20"
                    android:layout_marginRight="@dimen/dpDimen20"
                    android:background="@color/cela_bg"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_marginTop="@dimen/dpDimen20"
                android:layout_height="@dimen/dpDimen30">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dpDimen20"
                    android:text="会员等级"
                    android:textColor="#ff9e9e9e"
                    android:textSize="15sp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/dpDimen20"
                    android:text="V2"
                    android:textColor="#ffe8d2b0"
                    android:textSize="15sp"
                    />
                <View
                    android:layout_width="match_parent"
                    android:layout_alignParentBottom="true"
                    android:layout_height="@dimen/dpDimen1"
                    android:layout_marginLeft="@dimen/dpDimen20"
                    android:layout_marginRight="@dimen/dpDimen20"
                    android:background="@color/cela_bg"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_marginTop="@dimen/dpDimen20"
                android:layout_height="@dimen/dpDimen30">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dpDimen20"
                    android:text="团队等级"
                    android:textColor="#ff9e9e9e"
                    android:textSize="15sp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/dpDimen20"
                    android:text="沙和尚"
                    android:textColor="#ffe8d2b0"
                    android:textSize="15sp"
                    />
                <View
                    android:layout_width="match_parent"
                    android:layout_alignParentBottom="true"
                    android:layout_height="@dimen/dpDimen1"
                    android:layout_marginLeft="@dimen/dpDimen20"
                    android:layout_marginRight="@dimen/dpDimen20"
                    android:background="@color/cela_bg"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_marginTop="@dimen/dpDimen20"
                android:layout_height="@dimen/dpDimen30">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dpDimen20"
                    android:text="社区等级"
                    android:textColor="#ff9e9e9e"
                    android:textSize="15sp" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/dpDimen20"
                    android:text="一级站长"
                    android:textColor="#ffe8d2b0"
                    android:textSize="15sp"
                    />
                <View
                    android:layout_width="match_parent"
                    android:layout_alignParentBottom="true"
                    android:layout_height="@dimen/dpDimen1"
                    android:layout_marginLeft="@dimen/dpDimen20"
                    android:layout_marginRight="@dimen/dpDimen20"
                    android:background="@color/cela_bg"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dpDimen30"/>
        </LinearLayout>

    </LinearLayout>
</layout>
